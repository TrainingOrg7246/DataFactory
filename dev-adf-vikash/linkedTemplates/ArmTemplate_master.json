{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-adf-vikash"
		},
		"ls_AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "@linkedService().DatabricksExistingClusterId"
		},
		"ls_adls_param_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().adls_account_name}"
		},
		"ls_dev_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Az-keyVault-dev.vault.azure.net/"
		},
		"ls_dev_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Az-keyVault-dev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-adf-vikash_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_AzureDatabricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_adls_param_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_param_properties_typeProperties_url')]"
					},
					"ls_dev_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_dev_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_dev_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_dev_key_vault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}